{
    "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
    "grammars": [
        {
            "name": "tlb",
            "camelcase": "TLB",
            "title": "TL-B",
            "scope": "source.tlb",
            "file-types": ["tlb"],
            "injection-regex": "^tlb$",
            "class-name": "TreeSitterTlb"
        }
    ],
    "metadata": {
        "version": "0.1.0",
        "license": "MIT",
        "description": "TL-B grammar for tree-sitter",
        "authors": [
            {
                "name": "TON Core"
            },
            {
                "name": "TON Studio",
                "url": "https://tonstudio.io"
            }
        ],
        "links": {
            "repository": "https://github.com/ton-blockchain/ton-language-server"
        }
    },
    "bindings": {
        "c": true,
        "go": true,
        "java": false,
        "node": true,
        "python": false,
        "rust": true,
        "swift": false,
        "zig": false
    }
}
