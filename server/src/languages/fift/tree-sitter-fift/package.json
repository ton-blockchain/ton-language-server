{
    "name": "tree-sitter-fift",
    "version": "0.1.0",
    "description": "A tree-sitter grammar for the Fift assembly programming language",
    "repository": "https://github.com/ton-blockchain/ton-language-server",
    "license": "MIT",
    "author": {
        "name": "Petr Makhnef",
        "email": "mr.makhneff@gmail.com"
    },
    "main": "bindings/node",
    "types": "bindings/node",
    "keywords": [
        "incremental",
        "parsing",
        "tree-sitter",
        "fift"
    ],
    "files": [
        "grammar.js",
        "tree-sitter.json",
        "binding.gyp",
        "prebuilds/**",
        "bindings/node/*",
        "queries/*",
        "src/**",
        "*.wasm"
    ],
    "dependencies": {
        "node-addon-api": "^8.2.1",
        "node-gyp-build": "^4.8.2"
    },
    "devDependencies": {
        "tree-sitter-cli": "^0.25.0"
    },
    "peerDependencies": {
        "tree-sitter": "^0.21.1"
    },
    "peerDependenciesMeta": {
        "tree-sitter": {
            "optional": true
        }
    },
    "scripts": {
        "install": "node_modules/.bin/node-gyp-build",
        "prestart": "tree-sitter build --wasm",
        "start": "tree-sitter playground",
        "test": "node --test bindings/node/*_test.js"
    },
    "packageManager": "yarn@4.6.0"
}
