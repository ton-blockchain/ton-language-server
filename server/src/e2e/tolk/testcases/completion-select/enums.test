========================================================================
Enum member completion as expression
========================================================================
enum Color {
    Red = 10,
    Blue,
}

fun main() {
    Red<caret>
}
------------------------------------------------------------------------
enum Color {
    Red = 10,
    Blue,
}

fun main() {
    Color.Red<caret>
}

