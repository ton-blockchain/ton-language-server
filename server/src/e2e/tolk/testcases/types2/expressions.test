========================================================================
Paren expression
========================================================================
fun foo(): int | bool {}

fun main() {
    val a = (foo());
    //! ^ int | bool
    val a = ((((((((10))))))));
    //! ^ int
}
------------------------------------------------------------------------
ok

========================================================================
Tuple literal expression
========================================================================
fun main() {
    val a = [1, true, ""];
//!     ^ [int, bool, string]
}
------------------------------------------------------------------------
ok

========================================================================
Tensor literal type inference
========================================================================
fun main() {
    val data = (1, true, "hello");
    //! ^ (int, bool, string)
}
------------------------------------------------------------------------
ok

========================================================================
Complex tensor and tuple literal type inference
========================================================================
fun main() {
    val data = (1, [1, [1, true, "hello"], (1, [1, [1, true, "hello"], "hello"], "hello")], "hello");
    //! ^ (int, [int, [int, bool, string], (int, [int, [int, bool, string], string], string)], string)
}
------------------------------------------------------------------------
ok

========================================================================
As cast expression
========================================================================
fun main() {
    val a = true as int;
//!     ^ int
}
------------------------------------------------------------------------
ok

========================================================================
