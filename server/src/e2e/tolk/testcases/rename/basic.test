========================================================================
Global variable rename
========================================================================
global foo: int = 100;
//!    ^ BAR

fun test() {
    if (foo == 10) {
        throw foo;
    }
}

fun test2() {
    if (foo == 100) {
        throw foo + 200;
    }
}
------------------------------------------------------------------------
global BAR: int = 100;
//!    ^ BAR

fun test() {
    if (BAR == 10) {
        throw BAR;
    }
}

fun test2() {
    if (BAR == 100) {
        throw BAR + 200;
    }
}

